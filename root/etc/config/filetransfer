config filetransfer 'config'
	option upload_dir '/tmp/upload/'
	option max_file_size '100'
	option allowed_extensions 'ipk,tar,gz,zip,txt,conf,json,bin,img,sig,deb,rpm'
	option log_level 'info'
	option log_retention '30'
	option auto_clean '1'
	option enable_csrf '1'
	option enable_ssl '0'
	option allowed_ips ''
	option max_upload_sessions '5'
	option session_timeout '3600'
	option enable_file_validation '1'